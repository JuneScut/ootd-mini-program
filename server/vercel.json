{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ootd",
      "dest": "app.js",
      "headers": {
        "Cache-Control": "no-cache"
      },
      "timeout": 120
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
